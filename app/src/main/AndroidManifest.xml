            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAf_iLsCz9K4bhs1naFMhXDZ20FFXorWQg" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps"></activity>

    </application>

</manifest>